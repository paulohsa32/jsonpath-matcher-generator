{
  "name": "pathfinder",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.1.0",
  "bin": {
    "pathfinder": "./dist/cli.mjs"
  },
  "scripts": {
    "pathfinder": "jiti ./src/cli.ts",
    "dev": "nuxt dev web",
    "build": "unbuild",
    "build:nuxt": "nuxt build web",
    "test": "print '#TODO: add tests'",
    "lint": "eslint --ext .js,.ts . && prettier -c src test web",
    "lint:fix": "eslint --fix --ext .js,.ts . && prettier -w src test web",
    "prepack": "pnpm build",
    "prerelease": "pnpm lint && pnpm test && pnpm run prepack",
    "release": "pnpm prerelease && changelogen --release --push",
    "generate": "nuxt generate web",
    "preview": "nuxt preview web",
    "postinstall": "nuxt prepare web"
  },
  "dependencies": {
    "citty": "^0.1.1",
    "destr": "^1.2.2",
    "uuid": "^3.3.3",
    "copy-text-to-clipboard": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/tailwindcss": "^6.6.5",
    "@types/uuid": "^3.4.5",
    "changelogen": "^0.5.2",
    "consola": "^3.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-unjs": "^0.1.0",
    "jiti": "^1.18.2",
    "monaco-editor": "^0.37.1",
    "nuxt": "^3.4.0",
    "nuxt-icon": "^0.3.3",
    "nuxt-monaco-editor": "^1.2.2",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "typescript": "^4.3.2",
    "unbuild": "^1.2.0"
  }
}
